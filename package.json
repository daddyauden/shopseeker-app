{
    "name": "shopseeker",
    "version": "1.2.4",
    "private": true,
    "description": "ShopSeeker, delivery or pickup groceries in local store.",
    "author": "auden <auden@shopseeker.com>",
    "homepage": "https://www.shopseeker.com",
    "scripts": {
        "start": "react-native start",
        "ios": "npx react-native run-ios --simulator 'iPhone 13 Pro Max'",
        "install-ios": "npx react-native run-ios --device 'AudenX'",
        "bundle-ios": "npx react-native bundle --verbose --entry-file index.js --platform ios --dev false --reset-cache --bundle-output './ios/shopseeker/main.jsbundle' --assets-dest='ios/'",
        "android": "react-native run-android",
        "install-android": "npx react-native run-android --verbose --deviceId 'Z220180500018414'",
        "bundle-android": "npx react-native bundle --entry-file index.js --platform android --dev false --reset-cache --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res",
        "test": "jest",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "format": "prettier --tab-width 4 --write '**/*.js' '**/*.ts' '**/*.tsx' '**/*.json'",
        "push-notif": "xcrun simctl push AA3E0876-8866-48FE-92C1-644E343F24BB ./sample.apns"
    },
    "dependencies": {
        "@ant-design/icons-react-native": "^2.3.1",
        "@ant-design/react-native": "^4.2.0",
        "@apollo/client": "^3.3.15",
        "@invertase/react-native-apple-authentication": "^2.1.2",
        "@react-native-async-storage/async-storage": "^1.15.3",
        "@react-native-community/cameraroll": "^4.0.4",
        "@react-native-community/masked-view": "^0.1.10",
        "@react-native-community/segmented-control": "^2.2.2",
        "@react-native-community/slider": "^3.0.3",
        "@react-native-google-signin/google-signin": "^6.0.0",
        "@react-native-picker/picker": "^1.15.0",
        "@react-navigation/bottom-tabs": "^5.11.10",
        "@react-navigation/drawer": "^5.12.5",
        "@react-navigation/native": "^5.9.4",
        "@react-navigation/stack": "^5.14.4",
        "@stripe/stripe-react-native": "^0.1.4",
        "crc32": "^0.2.2",
        "currency.js": "^2.0.3",
        "dayjs": "^1.10.4",
        "graphql": "^15.5.0",
        "i18n-js": "^3.8.0",
        "isomorphic-unfetch": "^3.1.0",
        "parse": "3.2.0",
        "react": "17.0.1",
        "react-native": "0.64.0",
        "react-native-config": "^1.4.2",
        "react-native-config-reader": "^4.1.1",
        "react-native-crypto-js": "^1.0.0",
        "react-native-device-info": "^8.1.2",
        "react-native-fbsdk-next": "^4.2.0",
        "react-native-geolocation-service": "^5.3.0-beta.1",
        "react-native-gesture-handler": "^1.10.3",
        "react-native-image-crop-picker": "^0.36.2",
        "react-native-image-zoom-viewer": "^3.0.1",
        "react-native-keyboard-aware-scroll-view": "^0.9.4",
        "react-native-localize": "^2.0.3",
        "react-native-masked-text": "^1.13.0",
        "react-native-mmkv-storage": "^0.5.8",
        "react-native-notifications": "^3.4.2",
        "react-native-notifier": "^1.6.1",
        "react-native-pager-view": "^5.1.10",
        "react-native-picker-select": "^8.0.4",
        "react-native-reanimated": "^2.1.0",
        "react-native-root-toast": "^3.2.1",
        "react-native-safe-area-context": "^3.2.0",
        "react-native-screens": "^3.1.1",
        "react-native-scrollable-tab-view": "^1.0.0",
        "react-native-share": "^6.0.1",
        "react-native-swiper": "^1.6.0",
        "react-native-tab-view": "^3.0.1",
        "react-native-track-player": "^2.2.0-rc4",
        "react-native-underline-tabbar": "^1.3.6",
        "react-native-uuid": "^2.0.1",
        "react-native-vector-icons": "^8.1.0",
        "react-native-webview": "^11.4.4",
        "react-redux": "^7.2.3",
        "redux": "^4.0.5",
        "redux-enhancer-react-native-appstate": "^0.3.1",
        "redux-persist": "^6.0.0",
        "redux-saga": "^1.1.3",
        "socket.io-client": "^4.0.1",
        "strapi-sdk-javascript": "^0.3.3"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/plugin-proposal-decorators": "^7.13.15",
        "@babel/plugin-proposal-export-namespace-from": "^7.18.6",
        "@babel/runtime": "^7.12.5",
        "@react-native-community/eslint-config": "^2.0.0",
        "@types/jest": "^26.0.20",
        "@types/parse": "^2.18.6",
        "@types/react-native": "^0.64.0",
        "@types/react-native-vector-icons": "^6.4.6",
        "@types/react-test-renderer": "^16.9.2",
        "babel-jest": "^26.6.3",
        "babel-plugin-module-resolver": "^4.1.0",
        "detox": "^18.12.0",
        "eslint": "^7.14.0",
        "jest": "^26.6.3",
        "metro-react-native-babel-preset": "^0.64.0",
        "prettier": "^2.2.1",
        "react-test-renderer": "17.0.1",
        "reactotron-react-native": "^5.0.0",
        "reactotron-redux": "^3.1.3",
        "reactotron-redux-saga": "^4.2.3",
        "redux-immutable-state-invariant": "^2.1.0",
        "typescript": "^3.8.3"
    },
    "resolutions": {
        "@types/react": "^17"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "detox": {
        "configurations": {
            "ios.sim.debug": {
                "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/shopseeker.app",
                "build": "xcodebuild -workspace ios/shopseeker.xcworkspace -scheme shopseeker -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
                "type": "ios.simulator",
                "name": "iPhone X"
            },
            "ios.sim.release": {
                "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/shopseeker.app",
                "build": "xcodebuild -workspace ios/shopseeker.xcworkspace -scheme shopseeker -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
                "type": "ios.simulator",
                "name": "iPhone X"
            }
        }
    }
}
